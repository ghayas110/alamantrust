{
    "version": 2,
    "framework": "laravel",
    "builds": [
        {
            "src": "public/index.php",
            "use": "@vercel/php"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/(css|js|images)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/robots.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/(.*)",
            "dest": "public/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://alamantrust.vercel.app",
        "APP_KEY": "base64:7V9Y/9T7bV9Y/9T7bV9Y/9T7bV9Y/9T7bV9Y/9T7bV8="
    }
}
